#: ../quickinsert/widget.py:107
msgid "Direction:"
msgstr "Směr:"

#: ../quickinsert/widget.py:108
msgid "Up"
msgstr "Nahoru"

#: ../quickinsert/widget.py:108
msgid "Neutral"
msgstr "Neutrální"

#: ../quickinsert/widget.py:108
